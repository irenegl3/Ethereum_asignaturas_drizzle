tate={drizzleState} />\n\n\nconst CalificarV1 = ({drizzle, drizzleState}) => <article className=\"AppMisDatos\">\n    <h3>Calificar</h3>\n    <ContractData drizzle={drizzle} drizzleState={drizzleState}\n        contract={\"Asignatura\"} method={\"profesor\"} methodArgs={[]}\n        render={addr => {\n            if (addr !== drizzleState.accounts[0]) {\n                return <p>\"NO SOY EL PROFE\"</p>\n            }\n            return <ContractForm drizzle={drizzle} drizzleState={drizzleState}\n                                 contract=\"Asignatura\" method=\"califica\"\n                render={({inputs, inputTypes, state, handleInputChange, handleSubmit}) => {\n                    const labels = [\"Dirección del Alumno:\", \"Índice de la Evaluación:\",\n                        \"Tipo: (0=NP 1=Nota 2=MH):\", \"Nota (x10)\"];\n                    return <form onSubmit={handleSubmit}>\n                        {inputs.map((input, index) => (\n                            <p> {labels[index]} &nbsp;\n                                <input key={input.name} type={inputTypes[index]} name={input.name}\n                                       value={state[input.name]} placeholder={input.name}\n                                       onChange={handleInputChange}\n                                /> </p> ))}\n                        <button key=\"submit\" type=\"button\" onClick={handleSubmit}>calificar </button>\n                    </form>\n                }}\n            />\n        }}\n    />\n</article>\n\nconst CalificarV2 = ({drizzle, drizzleState}) => <article className=\"AppMisDatos\">\n    <h3>Calificar</h3>\n    <ContractData drizzle={drizzle} drizzleState={drizzleState}\n                  contract={\"Asignatura\"} method={\"profesor\"} methodArgs={[]}\n                  render={addr => {\n                      if (addr !== drizzleState.accounts[0]) {\n                          return <p>\"NO SOY EL PROFE\"</p>\n                      }\n                      return <ContractForm drizzle={drizzle} drizzleState={drizzleState}\n                                           contract=\"Asignatura\" method=\"califica\"\n                      />\n                  }}\n    />\n</article>\n\nconst CalificarV3 = ({drizzle, drizzleState}) => <article className=\"AppMisDatos\">\n    <h3>Calificar</h3>\n    <ContractData drizzle={drizzle} drizzleState={drizzleState}\n                  contract={\"Asignatura\"} method={\"profesor\"} methodArgs={[]}\n                  render={addr => {\n                      if (addr !== drizzleState.accounts[0]) {\n                          return <p>\"NO SOY EL PROFE\"</p>\n                      }\n                      return <ContractForm drizzle={drizzle} drizzleState={drizzleState}\n                                           contract=\"Asignatura\" method=\"califica\"\n                                           labels={[\"Dirección Alumno\", \"Índice Evaluación\",\n                                               \"0=NP 1=Nota 2=MH\", \"Nota (x10)\"]}\n                      />\n                  }}\n    />\n</article>\n\nexport default Calificar;\n","/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/MisCosas/MisDatos.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/MisCosas/MisNotas.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Calificaciones/CalificacionesBody.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Alumnos/AlumnosHead.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Evaluaciones/EvaluacionesBody.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Evaluaciones/EvaluacionesHead.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Evaluaciones/EvaluacionesSinComponentes.js",["119"],"import React from 'react';\n\nclass EvaluacionesSinComponentes extends React.Component {\n\n    state = {\n        ready: false,\n        evaluacionesLengthKey: null,\n        evaluacionesKeys: []\n    }\n\n    render() {\n        let evaluacionesLength;\n        let evaluaciones = [];\n\n        const {Asignatura} = this.props.drizzleState.contracts;\n        if (Asignatura && Asignatura.initialized) {\n\n            let el = Asignatura.evaluacionesLength[this.state.evaluacionesLengthKey];\n            evaluacionesLength = el ? el.value : 0;\n\n            for (let i = 0; i < this.state.evaluacionesKeys.length; i++) {\n                const eva = Asignatura.evaluaciones[this.state.evaluacionesKeys[i]];\n                evaluaciones[i] = eva ? eva.value : {nombre: \"??\", fecha: 0, puntos: 0};\n            }\n        }\n\n        let thead = <thead>\n        <tr>\n            <th>#</th>\n            <th>Nombre</th>\n            <th>Fecha</th>\n            <th>Puntos</th>\n        </tr>\n        </thead>;\n\n        let tbody = evaluaciones.map((evaluacion, index) =>\n            <tr key={index}>\n                <th>E<sub>{index}</sub></th>\n                <td>{evaluacion.nombre}</td>\n                <td>{evaluacion.fecha ? (new Date(1000 * evaluacion.fecha)).toLocaleString() : \"\"}</td>\n                <td>{(evaluacion.puntos / 10).toFixed(1)}</td>\n            </tr>);\n\n        return <section className=\"AppEvaluaciones\">\n            <h2>Evaluaciones Sin Componentes</h2>\n            <table>\n                {thead}\n                <tbody>{tbody}</tbody>\n            </table>\n        </section>\n    }\n\n    componentDidMount() {\n        this.setState({ready: true});\n    }\n\n    componentDidUpdate() {\n\n        const {Asignatura} = this.props.drizzleState.contracts;\n        if (!Asignatura || !Asignatura.initialized) return;\n\n        const instance = this.props.drizzle.contracts.Asignatura;\n\n        let changed = false;\n\n        // Copiar el estado\n        let {\n            evaluacionesLengthKey,\n            evaluacionesKeys\n        } = JSON.parse(JSON.stringify(this.state));\n\n        if (!evaluacionesLengthKey) { // Observar el metodo evaluacionesLength().\n            evaluacionesLengthKey = instance.methods.evaluacionesLength.cacheCall();\n            changed = true;\n        } else {\n            let el = Asignatura.evaluacionesLength[this.state.evaluacionesLengthKey];\n            el = el ? el.value : 0;\n            for (let i = evaluacionesKeys.length; i < el; i++) {\n                evaluacionesKeys[i] = instance.methods.evaluaciones.cacheCall(i);\n                changed = true;\n            }\n        }\n\n        if (changed) {\n            this.setState({\n                evaluacionesLengthKey,\n                evaluacionesKeys\n            });\n        }\n    }\n}\n\nexport default EvaluacionesSinComponentes;\n","/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Alumnos/AlumnoRow.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Calificaciones/CalificacionRow.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Evaluaciones/EvaluacionRow.js",[],"/Users/irene/Documents/BCDA-code/P3_Asignatura_Drizzle-main/dapp/src/components/Alumnos/AlumnosMatricula.js",["120"],"import { newContextComponents } from \"@drizzle/react-components\";\n\nconst {ContractData, ContractForm} = newContextComponents;\n\nconst AlumnosMatricula = (props) => {\n    const {drizzle, drizzleState} = props;\n    return (\n    <ContractData drizzle={drizzle} drizzleState={drizzleState}\n                  contract={\"Asignatura\"} method={\"profesor\"} methodArgs={[]}\n                  render={addr => {\n                      if (addr == drizzleState.accounts[0]) {\n                          return <p>\"SOY EL PROFE\"</p>\n                      } //comprobar si matriculado??\n                      return (  <div>\n                      <h3>Matricularse:</h3>\n                      <C